    "lint:fix": "eslint . --fix"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "eslint \"src/**/*.{ts,tsx}\"",
      "prettier --write"

    ]
  },
  "husky": {
